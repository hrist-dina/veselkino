$(document).ready(function(){

    $(document).on('click', '[js-training-load-more]', function(){

        var targetContainer = $('[js-training-list]'),
            url = $('[js-training-load-more]').attr('data-url');

        if (url !== undefined) {
            $.ajax({
                type: 'GET',
                url: url,
                dataType: 'html',
                success: function(data){

                    $('[js-training-load-more]').remove();

                    var elements = $(data).find('[js-training-item]'),
                        pagination = $(data).find('[js-training-load-more]');

                    targetContainer.append(elements);
                    targetContainer.after(pagination);
                }
            });
        }
    });
});
/* End */
