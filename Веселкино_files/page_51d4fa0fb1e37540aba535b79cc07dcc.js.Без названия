$(document).ready(function(){

    $(document).on('click', '[js-news-load-more]', function(){

        var targetContainer = $('[js-news-list]'),
            url = $('[js-news-load-more]').attr('data-url');

        if (url !== undefined) {
            $.ajax({
                type: 'GET',
                url: url,
                dataType: 'html',
                success: function(data){

                    $('[js-news-load-more]').remove();

                    var elements = $(data).find('[js-news-item]'),
                        pagination = $(data).find('[js-news-load-more]');

                    targetContainer.append(elements);
                    targetContainer.after(pagination);
                }
            });
        }
    });
});
/* End */
